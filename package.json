{
  "name": "clocks",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "clocks",
  "scripts": {
    "build": "webpack --env production",
    "dev": "webpack serve",
    "test": "jest"
  },
  "typings": "./types",
  "keywords": [
    "clock"
  ],
  "author": "Michael Beaton",
  "license": "ISC",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "tabWidth": 4,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard-scss",
    "rules": {
      "custom-property-empty-line-before": null,
      "color-hex-length": "long",
      "color-hex-case": "lower",
      "selector-id-pattern": null
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@swc/core": "^1.3.92",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "css-loader": "^6.8.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
